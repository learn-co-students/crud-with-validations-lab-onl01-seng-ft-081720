
<%= form_for @song do |f| %>
    <% if @song.errors.any? %>
        <% @song.errors.full_messages.each do |ms| %>
            <h5><%= ms %></h5>
        <% end %>
    <% end %>

    <%= f.text_field :title %>
    <%= f.check_box :released %>
    <%= f.text_field :release_year %>
    <%= f.text_field :artist_name%>
    <%= f.text_field :genre %>
    <%= f.submit %>
<% end %>