<h1>Edit Song</h1>
<%= form_for @song do |f| %>
<%= label_tag "Title:" %>
<%= f.text_field :title %><br><br>
<%= label_tag "Artist Name:" %>
<%= f.text_field :artist_name %><br><br>
<%= label_tag "Genre:" %>
<%= f.text_field :genre %><br><br>
<%= label_tag "Released:" %>
<%= f.check_box :released %><br><br>
<%= label_tag "Year of Release:" %>
<%= f.text_field :release_year %>
<br><br>
<%= f.submit %>
<% end  %>