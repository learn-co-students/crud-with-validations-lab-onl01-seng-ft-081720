<%= form_for @song do |f| %>
<% if @song.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@song.errors.count, "error") %>
      prohibited this song from being saved:
    </h2>
 
    <ul>
    <% @song.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

    Title <%= f.text_field :title %>
    Release Year <%= f.text_field :release_year %>
    Artist Name <%= f.text_field :artist_name %>
    Released? <%= f.text_field :released %>
    Genre <%= f.text_field :genre %>
    <%= f.submit %>
<% end %>



