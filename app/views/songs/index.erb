<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Release Year</th>
            <th>Artist</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
    <%= @songs.each do |song| %>
        <tr>
            <td><%= link_to song.title, song_path(song) %></td>
            <td><%= song.released ? song.release_year : "Not released" %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
    <% end %>
    </tbody>
</table>