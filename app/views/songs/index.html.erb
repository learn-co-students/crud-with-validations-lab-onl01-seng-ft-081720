
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Released?</th>
            <th>Release_year</th>
            <th>Artist_name</th>
            <th>Genre</th>
    </tr>
    </thead>
    <tbody><%= @songs.each do |song| %>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.release_year %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
    <% end %></tbody>
</table>
