<%= form_for @song do |f|%>
<%= label_tag "Title:" %>
<%= f.text_field :title %>

<%= label_tag "Released:" %>
<%= f.check_box :released %>

<%= label_tag "Release Year:" %>
<%= f.text_field :release_year %>

<%= label_tag "Artist Name:" %>
<%= f.text_field :artist_name %>

<%= label_tag "Genre:" %>
<%= f.text_field :genre %>

<%= f.submit %>
 <% end %>
